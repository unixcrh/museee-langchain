  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start:dev": "DB_HOST=localhost ./node_modules/.bin/pm2 start ecosystem.config.cjs && npm run pm2:log",
    "start:prod": "DB_HOST=fxdemo.museee.com ./node_modules/.bin/pm2 start ecosystem.config.cjs && npm run pm2:log",
    "start": "./node_modules/.bin/pm2 start ecosystem.config.cjs",
    "stop": "./node_modules/.bin/pm2 stop all",
    "pm2:log": "./node_modules/.bin/pm2 logs -f --lines 200",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "ingest": "tsx -r dotenv/config scripts/ingest-data.ts"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@pinecone-database/pinecone": "0.0.12",
    "@radix-ui/react-accordion": "^1.1.1",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "json-serve": "^0.1.0",
    "json-server": "^0.17.3",
    "langchain": "0.0.55",
    "lucide-react": "^0.125.0",
    "next": "13.2.3",
    "pdf-parse": "1.1.1",
    "pm2": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "react-modal": "^3.16.1",
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.0",
    "@types/formidable": "^2.0.6",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "formidable": "^2.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "starter",
    "gpt4",
    "pinecone",
    "typescript",
    "nextjs",
    "langchain",
    "pdf",
    "openai"
